{
  "name": "cat-coding",
  "displayName": "Cat Coding",
  "description": "A cat that codes",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cat-coding.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "catCoding.start",
        "title": "Start new Cat Coding session",
        "category": "Cat Coding"
      },
      {
        "command": "catCoding.yarn",
        "title": "Give Yarn ðŸ§¶",
        "category": "Cat Coding"
      },
      {
        "command": "catCoding.doRefactor",
        "title": "Do Refactor",
        "category": "Cat Coding"
      }
    ],
    "menus": {
      "webview/context": [
        {
          "command": "catCoding.yarn",
          "when": "webviewId == 'catCoding'"
        },
        {
          "command": "catCoding.doRefactor",
          "when": "webviewId == 'catCoding'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.109.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}